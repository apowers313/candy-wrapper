{
    "tags": {
        "allowUnknownTags": false
    },
    "opts": {
        "template": "./node_modules/docdash",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "./docs",
        "recurse": true
    },
    "source": {
        "include": "./candy-wrapper.js"
    },
    "docdash": {
        "static": true,
        "sort": true
    },
    "plugins": [
        "plugins/markdown"
    ]
}